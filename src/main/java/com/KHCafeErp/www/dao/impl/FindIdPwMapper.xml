<mapper namespace="com.KHCafeErp.www.dao.face.FindIdPwDao">

	<select id="findid" parameterType="com.KHCafeErp.www.dto.Staff" resultType="com.KHCafeErp.www.dto.Staff">
		select staffNo from staff
